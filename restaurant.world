<?xml version="1.0" ?><sdf version="1.6">  <world name="default">    <!-- 优化后的物理求解器设置 -->    <physics type="ode">      <gravity>0 0 -9.81</gravity>      <ode>        <solver>          <type>quick</type>          <!-- 迭代次数提升至 100 -->          <iters>50</iters>          <!-- 松弛因子提升至 1.5 -->          <sor>1.5</sor>        </solver>        <constraints>          <!-- 降低 CFM（更刚硬），提高 ERP（更积极修正穿透） -->          <cfm>0.0</cfm>          <erp>0.8</erp>          <contact_max_correcting_vel>100.0</contact_max_correcting_vel>          <contact_surface_layer>0.0</contact_surface_layer>        </constraints>      </ode>      <!-- 保持高更新率与小步长以提升稳定性 -->      <real_time_update_rate>1000</real_time_update_rate>      <max_step_size>0.001</max_step_size>    </physics>    <plugin name="gazebo_link_attacher" filename="libgazebo_link_attacher.so"/>    <plugin name="gazebo_ros_state" filename="libgazebo_ros_state.so">      <ros>        <namespace>/gazebo</namespace>      </ros>      <update_rate>30.0</update_rate>  <!-- 自定义发布频率 -->    </plugin>    <!-- 基础场景模型 -->    <include>      <uri>model://ground_plane</uri>    </include>    <include>      <uri>model://sun</uri>    </include>    <!-- 餐厅模型 -->    <include>      <uri>model://RestaurantModel</uri>      <pose>-4.87 -6.19 0 0 0 0</pose>    </include>    <!-- 雪碧模型分布 -->    <include>      <name>sprite_table0</name>      <uri>model://sprite</uri>      <pose>1.68 -4.869 0.86 0 0 0</pose>    </include>    <include>      <name>sprite_table1</name>      <uri>model://sprite</uri>      <pose>-0.098 -4.869 0.86 0 0 0</pose>    </include>    <include>      <name>sprite_table2</name>      <uri>model://sprite</uri>      <pose>-1.876 -4.869 0.86 0 0 0</pose>    </include>    <include>      <name>sprite_table3</name>      <uri>model://sprite</uri>      <pose>-3.654 -4.869 0.86 0 0 0</pose>    </include>    <include>      <name>sprite_table4</name>      <uri>model://sprite</uri>      <pose>-1.876 -1.864 0.86 0 0 0</pose>    </include>    <include>      <name>sprite_table9</name>      <uri>model://sprite</uri>      <pose>-0.043 5.44 0.86 0 0 0</pose>    </include>    <include>      <name>sprite_counter1</name>      <uri>model://sprite</uri>      <pose>1.839 -2.315 1.168 0 0 0</pose>    </include>  </world></sdf>