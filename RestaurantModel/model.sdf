<!-- RestaurantModel/model.sdf --><?xml version="1.0"?><sdf version="1.6">  <model name="RestaurantModel">    <static>true</static>    <pose>0 0 0 0 0 0</pose>    <link name="link">      <visual name="visual">        <geometry>          <mesh>            <uri>model://RestaurantModel/meshes/Burgerking.obj</uri>          </mesh>        </geometry>        <!-- 若需调整视觉大小，可添加 scale -->        <!-- <scale>1 1 1</scale> -->      </visual>      <collision name="collision">        <geometry>          <mesh>            <uri>model://RestaurantModel/meshes/Burgerking.obj</uri>          </mesh>        </geometry>        <surface>          <!-- 增加摩擦刚度 -->          <friction>            <ode>              <mu>0.6</mu>              <mu2>0.6</mu2>              <kp>200000</kp>              <kd>10.0</kd>            </ode>          </friction>          <!-- 提高接触刚度 / 减少穿透 -->          <contact>            <ode>              <soft_cfm>0.001</soft_cfm>              <soft_erp>0.8</soft_erp>      	      <contact_max_correcting_vel>50.0</contact_max_correcting_vel>      	      <min_depth>0.001</min_depth>            </ode>          </contact>        </surface>      </collision>    </link>  </model></sdf>